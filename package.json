{
  "name": "@oxide/design-system",
  "version": "0.0.3",
  "description": "Home of reusable design assets and token for oxide internal sites",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build:js": "tsc",
    "build:themes": "./scripts/build_themes.sh",
    "build:css:asciidoc": "postcss ./src/css/asciidoc.css --dir ./dist/css",
    "build": "run-s build:*",
    "prebuild:tokens": "token-transformer --preserveRawValue ./styles/src/tokens.json ./styles/src/.tokens/main.json global,colors,core,main",
    "release": "auto shipit"
  },
  "author": "Oxide Computer Company <bots@oxidecomputer.com>",
  "license": "MPL 2.0",
  "dependencies": {
    "@djencks/asciidoctor-mathjax": "^0.0.8",
    "asciidoctor": "^2.2.6",
    "highlight.js": "^11.6.0",
    "style-dictionary": "^3.7.1",
    "token-transformer": "^0.0.26",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/react": "^18.0.18",
    "auto": "^10.37.6",
    "autoprefixer": "^10.4.12",
    "cssnano": "^5.1.13",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.23.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "postcss-nesting": "^10.2.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "tailwindcss": "^3.1.8",
    "ts-dedent": "^2.2.0"
  },
  "files": [
    "styles/dist/*",
    "dist/**/*",
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxidecomputer/design-system.git"
  },
  "bugs": {
    "url": "https://github.com/oxidecomputer/design-system/issues"
  },
  "homepage": "https://github.com/oxidecomputer/design-system#readme",
  "auto": {
    "plugins": [
      "npm",
      "released"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
